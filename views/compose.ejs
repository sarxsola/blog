<%- include("partials/header") -%>


<h1>Compose</h1>

<form class="item" action="/compose" method="POST">

    <div class="form-group">
        <label for="postTitle">Title</label>
        <input class="form-control"type="text" name="postTitle" id="postTitle">
    </div>
    
    <div class="form-group">
        <label for="postContent">Post</label>
        <textarea class="form-control" rows="4" type="text" name="postBody" id="postBody"></textarea>
    </div>
    
    <button class="btn btn-primary"type="submit" name="button">Publish</button>

</form>



<%- include("partials/footer") -%>